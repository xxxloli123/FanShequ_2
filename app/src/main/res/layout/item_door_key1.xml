<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical">

    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/cell_item_layout"
        android:layout_width="match_parent"
        android:layout_height="120px"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20px"
            android:focusable="false"
            android:padding="10px"
            android:src="@mipmap/xiaoqubiaozi" />

        <TextView
            android:id="@+id/cell_name"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="10px"
            android:layout_weight="1"
            android:ellipsize="end"
            android:focusable="false"
            android:gravity="center_vertical"
            android:text="cell_name"
            android:textColor="@color/text_3"
            android:textSize="38px"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/door_next_img"
            android:layout_width="65px"
            android:layout_height="65px"
            android:focusable="false"
            android:layout_marginRight="10px"
            android:tint="@color/text_9"
            android:src="@mipmap/ico_qj" />

    </com.zhy.autolayout.AutoLinearLayout>

   <!-- <ListView
        android:id="@+id/key_list_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10px"
        android:layout_marginRight="10px"
        android:visibility="gone"
        android:dividerHeight="1px"
        android:divider="@color/devider_color"/>-->
</com.zhy.autolayout.AutoLinearLayout>


